{
    "listeners": {
        "*:8080": {
            "pass": "applications/my-app"
        }
    },
    "applications": {
        "my-app": { 
            "type": "php",
            "processes": 2,
            "root": "/var/www/app/",
            "index": "index.php",
            "user": "nobody",
            "group": "nogroup"
        }
    }
}

